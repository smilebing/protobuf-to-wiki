<idea-plugin>
  <id>com.smilepig.protobuf-to-wiki</id>
  <name>protobuf-to-wiki</name>
  <version>1.0</version>
  <vendor email="iblandshot@gmailcom" url="http://www.zhudahe.cn">smilepig</vendor>

  <description><![CDATA[
      把生成protobuf对应的接口文档<br>
    ]]></description>

  <change-notes><![CDATA[
      v1.0 <br>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="MyAction" class="MyAction" text="test" description="test">
      <add-to-group group-id="ToolsMenu" anchor="first"/>
    </action>
    <action id="QQPlugin.ProtoToWiki" class="com.smilepig.ProtobufToWikiAction" text="ProtobufToWiki"
            description="自动生成protobuf对应的接口文档">
      <add-to-group group-id="EditMenu" anchor="after" relative-to-action="$Redo"/>
      <add-to-group group-id="CodeMenu" anchor="after" relative-to-action="Generate"/>
      <keyboard-shortcut keymap="$default" first-keystroke="meta P"/>
    </action>

    <action class="com.smilepig.PsiNavigationDemoAction" id="PsiNavigationDemo"
            text="PSI Navigation Demo...">
      <add-to-group group-id="ToolsMenu" anchor="last"/>
    </action>
  </actions>

</idea-plugin>